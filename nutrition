#Create dictionary with all fruits and its calories
fruits = {
    "apple":130,
    "avocado":50,
    "banana":110,
    "cantaloupe":50,
    "grapefruit":90,
    "grapefruit":60,
    "grapes":90,
    "honeydew lemon":50,
    "kiwifruit":90,
    "lemon":15,
    "lime":20,
    "nectarine":60,
    "orange":80,
    "peach":60,
    "pear":100,
    "pineapple":50,
    "plums":70,
    "strawberries":50,
    "sweet cherries":100,
    "tangerine":50,
    "watermelon":80,
}

# Get user input
fruit_asked = input("Item: ")


#Loop through fruits dictionary
for key in fruits:

    # Find the fruit asked (remember to user lowercase)
    if key == fruit_asked.lower():
       # Print fruit's calories
       print("Calories:", fruits[key])
